{
  "family": "book-ms-app",
    "containerDefinitions": [
        {
            "name": "book-ms-webapp",
      "image": "public.ecr.aws/z7d7p9s0/marshallinc:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "name": "book-ms-webapp-80-tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "DB_PWD",
                    "value": "marshall93"
                },
                {
                    "name": "DB_HOST",
                    "value": "sourab-db-1.c7qy06y0snr7.ap-southeast-2.rds.amazonaws.com"
                },
                {
                    "name": "DB_PORT",
                    "value": "5432"
                },
                {
                    "name": "DB",
                    "value": "books"
                },
                {
                    "name": "DB_USER",
                    "value": "postgres"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/book-ms-app",
                    "awslogs-create-group": "true",
                    "awslogs-region": "ap-southeast-2",
                    "awslogs-stream-prefix": "ecs"
        }
      }
        }
    ],
    "executionRoleArn": "arn:aws:iam::637423368177:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
  "memory": "3072"
}
